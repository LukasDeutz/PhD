mendeley-groups = {neuroscience,neuroscience/correaltions,neuroscience/progress report},
number = {5965},
nopages = {587--590},
pmid = {20110507},
title = {{The asynchronous state in cortical circuits}},
nourl = {http://science.sciencemag.org/content/327/5965/587.short},
volume = {327},
year = {2010}
